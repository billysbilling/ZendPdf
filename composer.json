{
    "name": "zendframework/zendpdf",
    "description": "Zend Pdf Component, updated to be compatible with PHP 8.1",
    "type": "library",
    "license": "BSD-3-Clause",
    "config": {
        "secure-http": false
    },
    "keywords": [
        "zf2",
        "pdf"
    ],
    "homepage": "http://packages.zendframework.com/",
    "autoload": {
        "psr-0": {
            "ZendPdf\\": "library/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/",
            "secure-http": false
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "zendframework/zend-memory": ">=2.0.0",
        "zendframework/zend-stdlib": ">=2.0.0",
        "phpunit/phpunit": "^9.5"
    }
}
