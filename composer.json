{
    "name": "billysbilling/zendpdf",
    "description": "Zend Pdf Component, updated to be compatible with PHP 8.1",
    "type": "library",
    "license": "BSD-3-Clause",
    "config": {
        "secure-http": false
    },
    "keywords": [
        "zf2",
        "pdf"
    ],
    "homepage": "http://packages.zendframework.com/",
    "autoload": {
        "psr-0": {
            "ZendPdf\\": "library/"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "phpunit/phpunit": "^9.5",
        "laminas/laminas-memory": "^2.5",
        "laminas/laminas-stdlib": "^3.6"
    }
}
